<h1>Listing People</h1>
<% @people.each do |person| %>
  <tr>
    <td><%=h person.id %></td>
    <td><%= link_to 'Show', person_path(person.id) %></td>
    <td><%= link_to 'Edit', edit_person_path(person.id) %></td>
    <td><%= link_to 'Destroy', person_path(person.id), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%= link_to 'New person', new_person_path %><br/><br/>
<%= link_to 'next section', job_applications_next_path(:applicant => @applicant.id) %>
